    "publish": "yarn run prepublish && npm publish --access public && rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxzaleski/useTimer.git"
  },
  "keywords": [
    "react-hook",
    "use-timer",
    "react-timer"
  ],
  "author": "Maximilien Zaleski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maxzaleski/useTimer/issues"
  },
  "homepage": "https://github.com/maxzaleski/useTimer#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/jest": "^28.1.5",
    "@types/react": "^18.0.15",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^28.1.3",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^4.7.4"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  }
}
